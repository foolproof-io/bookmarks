{
  "manifest_version": 2,

  "name": "Foolproof Bookmarks",
  "description": "This extension allows the user to bookmark a webpage with an extension.",
  "version": "0.1",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "glowing-man.png"
  },
  "permissions": [
    "activeTab",
    "identity"
  ],
  "oauth2": {
    "client_id": "163096248652-e0joed62i9ub72fhil1eokv4oiv1t26n.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  }
}

